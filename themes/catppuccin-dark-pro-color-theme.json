{
  "name": "Catppuccin Dark Pro",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "annotation:dart": {
      "foreground": "#fab387"
    },
    "enumMember": {
      "foreground": "#94e2d5"
    },
    "macro": {
      "foreground": "#fab387"
    },
    "memberOperatorOverload": {
      "foreground": "#cba6f7"
    },
    "parameter.label:dart": {
      "foreground": "#a6adc8"
    },
    "property:dart": {
      "foreground": "#fab387"
    },
    "tomlArrayKey": {
      "foreground": "#f9e2af"
    },
    "variable:dart": {
      "foreground": "#fab387"
    },
    "variable.constant": {
      "foreground": "#fab387"
    },
    "variable.defaultLibrary": {
      "foreground": "#f9e2af"
    },
    "class": {
      "foreground": "#f9e2af"
    },
    "function": {
      "foreground": "#89b4fa"
    },
    "keyword": {
      "foreground": "#cba6f7"
    },
    "parameter": {
      "foreground": "#f38ba8"
    },
    "property": {
      "foreground": "#fab387"
    },
    "string": {
      "foreground": "#a6e3a1"
    },
    "type": {
      "foreground": "#f9e2af"
    },
    "variable": {
      "foreground": "#f38ba8"
    },
    "variable.readonly": {
      "foreground": "#fab387"
    }
  },  "tokenColors": [
    // Comments - subtle and elegant
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7f849c"
      }
    },

    // Strings - green harmony from Catppuccin
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },

    // Numbers - warm peach
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#fab387"
      }
    },

    // Keywords - purple, maintaining One Dark Pro's scope but with Catppuccin colors
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },

    // Functions - blue harmony
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },

    // Classes and Types - yellow harmony
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },

    // Variables and Parameters - red/pink
    {
      "name": "Variables and Parameters",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.readwrite",
        "variable.other.object",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#f38ba8"
      }
    },

    // Properties and Attributes - orange
    {
      "name": "Properties and Attributes",
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "entity.other.attribute-name",
        "meta.object.member",
        "meta.field.declaration"
      ],
      "settings": {
        "foreground": "#fab387"
      }
    },

    // HTML/XML Tags - red
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#f38ba8"
      }
    },

    // CSS Selectors - purple
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "source.css entity.name.tag",
        "source.sass entity.name.tag",
        "source.scss entity.name.tag",
        "source.less entity.name.tag",
        "source.stylus entity.name.tag"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },

    // CSS Properties - blue
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.sass",
        "support.type.property-name.less"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },

    // CSS Values - teal
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.font-name.css",
        "support.constant.media.css",
        "support.constant.color.w3c-standard-color-name.css",
        "constant.other.color.rgb-value.css",
        "constant.other.rgb-value.css"
      ],
      "settings": {
        "foreground": "#94e2d5"
      }
    },

    // JavaScript/TypeScript specifics
    {
      "name": "JS/TS Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.default",
        "meta.import",
        "meta.export"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },

    // React/JSX specifics
    {
      "name": "JSX Component Names",
      "scope": [
        "support.class.component.jsx",
        "support.class.component.tsx",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },

    // JSON specifics
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.json > string.quoted.json"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },

    // Python specifics
    {
      "name": "Python Decorators",
      "scope": [
        "entity.name.function.decorator.python",
        "punctuation.definition.decorator.python"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },

    // Python f-strings
    {
      "name": "Python f-strings",
      "scope": [
        "string.quoted.single.python.f",
        "string.quoted.double.python.f"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },

    // Markdown specifics
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#f38ba8",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },

    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },

    // Git specifics
    {
      "name": "Git Keywords",
      "scope": [
        "keyword.other.git"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },

    // =============================================================================
    // C/C++ Language Specific Syntax Highlighting
    // =============================================================================

    // C/C++ Preprocessor directives - bright orange for visibility
    {
      "name": "C/C++ Preprocessor Directives",
      "scope": [
        "keyword.control.directive.define.c",
        "keyword.control.directive.define.cpp",
        "keyword.control.directive.include.c",
        "keyword.control.directive.include.cpp",
        "keyword.control.directive.pragma.c",
        "keyword.control.directive.pragma.cpp",
        "keyword.control.directive.conditional.c",
        "keyword.control.directive.conditional.cpp",
        "keyword.control.directive.undef.c",
        "keyword.control.directive.undef.cpp",
        "keyword.control.directive.line.c",
        "keyword.control.directive.line.cpp",
        "keyword.control.directive.diagnostic.c",
        "keyword.control.directive.diagnostic.cpp",
        "meta.preprocessor.include.c",
        "meta.preprocessor.include.cpp",
        "meta.preprocessor.macro.c",
        "meta.preprocessor.macro.cpp",
        "meta.preprocessor.conditional.c",
        "meta.preprocessor.conditional.cpp",
        "meta.preprocessor.pragma.c",
        "meta.preprocessor.pragma.cpp"
      ],
      "settings": {
        "foreground": "#fab387",
        "fontStyle": "italic"
      }
    },

    // C/C++ Preprocessor macro names - yellow
    {
      "name": "C/C++ Preprocessor Macro Names",
      "scope": [
        "entity.name.function.preprocessor.c",
