    {
      "name": "C++ Namespace Names",
      "scope": [
        "entity.name.namespace.cpp",
        "entity.name.scope-resolution.namespace.cpp",
        "meta.using-namespace.cpp"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
